<Window x:Class="Test4.Details"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Test4"
        mc:Ignorable="d"
        Title="Details" Height="450" Width="800">
    <Grid>
        <ListBox Name="lstdetails">
            <TextBlock Text="{Binding Kaufpreis}"/>
            <TextBlock Text="{Binding Makler}"/>
            <TextBlock Text="{Binding Notar}"/>
        </ListBox>
        
    </Grid>
</Window>
